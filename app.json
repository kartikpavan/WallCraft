{
  "expo": {
    "name": "wallcraft",
    "slug": "wallcraft",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon2.png",
      "resizeMode": "contain",
      "backgroundColor": "#121c22"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon2.png",
        "backgroundColor": "#121c22"
      },
      "package": "com.kartikpavan2000.wallcraft"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Skatone.ttf",
            "./assets/fonts/Inter-Regular.ttf",
            "./assets/fonts/Inter-SemiBold.ttf",
            "./assets/fonts/Inter-Bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6319a684-3251-4d40-9467-30d7cdbb3fbe"
      }
    }
  }
}
